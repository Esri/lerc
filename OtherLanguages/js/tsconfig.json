{
  "compilerOptions": {
    "module": "ES6",
    "lib": ["ES2020", "ES2020.Promise", "DOM", "scripthost"],
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es6",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "importHelpers": false,
    "moduleResolution": "node",
    "strictNullChecks": true
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": []
}