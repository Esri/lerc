{
  "name": "lerc",
  "version": "4.1.0",
  "browser": "LercDecode.js",
  "type": "module",
  "bugs": {
    "url": "https://github.com/esri/lerc/issues"
  },
  "description": "Rapid decoding of Lerc compressed raster data for any standard pixel type.",
  "keywords": [
    "Esri",
    "LERC"
  ],
  "author": "Esri <dev_tools@esri.com> (http://developers.arcgis.com)",
  "contributors": [
    "Thomas Maurer",
    "Wenxue Ju"
  ],
  "exports": {
    ".": {
      "require": "./LercDecode.js",
      "import": "./LercDecode.es.js",
      "default": "./LercDecode.js"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~8.49.0",
    "@typescript-eslint/parser": "~8.49.0",
    "eslint": "~9.39.1",
    "eslint-plugin-prettier": "~5.5.4",
    "grunt": "~1.6.1",
    "grunt-contrib-clean": "~2.0.1",
    "grunt-contrib-concat": "~2.1.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-eslint": "~26.0.0",
    "grunt-rollup": "~12.0.0",
    "grunt-terser": "~2.0.0",
    "prettier": "~3.7.4",
    "rollup": "~4.53.3",
    "terser": "~5.44.1",
    "typescript": "~5.9.3"
  },
  "homepage": "https://github.com/Esri/lerc",
  "license": "Apache-2.0",
  "main": "LercDecode.js",
  "module": "LercDecode.es.js",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/Esri/lerc.git"
  },
  "scripts": {
    "build": "npm run ts && grunt clean && grunt dist --format=umd && grunt dist --format=es && npm run tests:sanity",
    "dev": "npm run ts && grunt clean && grunt --format=umd && npm run tests:sanity",
    "tests": "npm run tests:sanity && npm run tests:level2",
    "tests:sanity": "node tests/sanity.mjs",
    "tests:level2": "node tests/level2.mjs",
    "lint": "grunt eslint",
    "ts": "tsc"
  }
